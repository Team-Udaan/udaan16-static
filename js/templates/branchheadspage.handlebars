<div>

  <div class="row">
    <div class="col s12 m6">
      <h3 class="center">Branch Heads</h3>
      {{#each heads}}
        <div class="card">
          <div class="card-content">
            <span class="card-title">{{this.name}}</span>
            <p>{{this.mobile}}</p>
          </div>
        </div>
      {{/each}}
    </div>
    <div class="col s12 m6">
      <h3 class="center">Branch Co-Heads</h3>
      {{#each coheads}}
        <div class="card">
          <div class="card-content">
            <span class="card-title">{{this.name}}</span>
            <p>{{this.mobile}}</p>
          </div>
        </div>
      {{/each}}
    </div>
  </div>

</div>